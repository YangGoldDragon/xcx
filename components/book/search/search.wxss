/* components/book/search/search.wxss */

.search-container {
  width: 100%;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}

.inner {
  height: 34px;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 50px;
  background-color: #f5f5f5;
}

.search-icon {
  margin-right: 10px;
  width: 14px;
  height: 14px;
}

.search-text {
  color: #999;
  font-size: 12px;
  line-height: 1;
}
